.head{
  background-color:rgba(0,0,0,0.4);
  width:60%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  font-size:50rpx;
  border-top-left-radius:25rpx;
  border-top-right-radius:25rpx;
  color:white;
  font-weight:600;
}
.list{display: table;width: 100%;background-color: rgba(0, 0, 0, 0.1);color:#e3e3e3}
.row{display: table-row;height: 110rpx}
.row:nth-child(odd){  background-color: rgba(0, 0, 0, 0.2)}
.mine{font-size: 38rpx;font-weight: 600;background-color: rgba(3,3,3,0.4) !important}
.cell{text-align: center;display: table-cell;justify-content: center;align-items: center;vertical-align: middle;}
.rank{font-size:40rpx;font-weight: 600;padding-left:20rpx}
.name,.score{text-align: left}
.headpic{
  width:80rpx;
  border-radius:50%;
  margin:auto;
  margin-top: 11rpx;
  }